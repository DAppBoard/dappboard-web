<div id="contracts-token-page-nfts" class="columns" id="contracts-token">
  <div class="column is-2">
    <%- include('menu'); %>
  </div>
  <div class="column is-10 general_background_normal ">
    <div class="box" style="margin:10px;">
      <nav class="level">
  <!-- Left side -->
  <div class="level-left">
    <div class="level-item">
      <h1 class="title">Token user information</h1>
    </div>
  </div>

  <!-- Right side -->
  <div class="level-right">
    <p class="level-item">    <input type="text" id="tokendaterange" class="input" style="  float: right;"  />
</p>
  </div>
</nav>

    <hr>
    <canvas id="chart_nfts"></canvas>
    <v-server-table ref="table" url="/api/tokens/<%= contracts.contract.address %>/nfts" @loaded="onLoaded" :columns="columns" :options="options">
      <p slot="address" slot-scope="props" class="table_center_title">
        {{ props.row.nft}}
      </p>
      <center slot="transfers" slot-scope="props">
        <strong>{{ props.row.transfers }}</strong>
      </center>
    </v-server-table>

</div>
  </div>
</div>
<%- /* Expose locals as `window.SAILS_LOCALS` :: */ exposeLocalsToBrowser() %>
